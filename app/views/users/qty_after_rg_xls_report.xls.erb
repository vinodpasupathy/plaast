<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Issue Date</Data></Cell>
        <Cell><Data ss:Type="String">Qty After Rg</Data></Cell>
   </Row>
       <% $qt.each do |i| %>
       <Row>
        <Cell><Data ss:Type="String"><%= i[0]%></Data></Cell>
        <Cell><Data ss:Type="String"><%=(i[1].to_f).round(3)%></Data></Cell>
       </Row>
    <%end%>
      <Row></Row>

       <Row>
       <Cell><Data ss:Type="String">Total</Data></Cell>
       <Cell><Data ss:Type="String"><%= $qtt.to_f.round(4)%></Data></Cell>
      </Row>
    </Table>
  </Worksheet>
</Workbook>

