<div class="bod">
  <div class="col-md-10 col-sm-8 body-wrap">
<div class="set">
<div class="body-container">
<div class="body-header">
Trash - Order Summary
</div>
<div class="body-content">
<div class="table-container">
<div class="header">
    List of Order 
</div>
<table class="table">
<thead>
<tr>
<th>Order No</th>
<th>Party</th>
<th>Coloring Agent</th>
<th colspan="3"><center>Actions</center></th>
</tr>
</thead>
<tbody>
<ul id = "order_summary" >
<% @order_summary.each do |i| %>
<tr>
<td><%= i.order_no%></td>
<td><%= i.party %></td>
<td><%= i.chemicals%></td>
<td><%= link_to image_tag("/assets/trash1.png", height: '20', width: '20',:class=>"img-circle",:title => 'Remove', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'delete_trashOrderSummary', :id => i.id }%>
</td>
<td><center><%= link_to   users_trash_showOrderSummary_path(:id => i.id) ,class: "btn btn-default",:title => 'Show', :onclick => 'return openPopup(this);', :rel => 'tooltip' do%><i class="fa fa-eye"></i><%end%>
</center></td>

<td><%= link_to image_tag("/assets/restore.png", height: '20', width: '20',:class=>"img-circle",:title => 'Restore', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'restore_trashOrderSummary', :id => i.id }%>
</td>

</tr>
<%end%>
</ul>
</tbody>
</table>
</div>
</div>
<div class="digg_pagination" align="right">
    <%= will_paginate @order_summary1, :container => false %>
</div>
</div>
</div>
</div>
