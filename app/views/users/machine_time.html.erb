<div class = "bod">
<div class="col-md-10 col-sm-8 body-wrap">
<div class="sett">
<div class="body-container">
<div class="body-header">
Cycle Time
</div>
<div class="body-content">
<div class="hit">
<div class="header">
List of Mould & Cycle Time
</div>
<div class="table-container">
<table class="table">
<thead>
<tr>
<th>Mould No</th>
<th>Cycle Time</th>
<th>Action</th>
</tr>
</thead>
<tbody>
<%= form_for([@machine_time], validate: true, :url=>{:controller=>"users", :action => "machine_time_create"}) do |i|%>
<tr class="odd">

<td><%= i.collection_select(:mo_no, MouldNo.select(:mould_no_list).order('mould_no_list ASC').uniq, :mould_no_list, :mould_no_list,{:id=>'mo_no_select'},:prompt=> "--select--") %> </td>
<td><%= i.number_field :cycle_time,:require=>"true"%></td>
<td><%= i.submit "Submit",:class=>"btn btn-info btn-sm"%></td></tr>
<%end%>
<% @mach.each do|i|%>
<tr class="even">
<td><%= i.mo_no %></td>
<td><%= i.cycle_time %></td>
<td><%= link_to image_tag("/assets/trash1.png", height: '20', width: '20',:class=>"img-circle",:title => 'Delete', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'machine_time_delete',:id => i.id }%></td></tr>
<%end%>
</tbody>
</table>
</div>
</div>
</div>
</div>
</div>

