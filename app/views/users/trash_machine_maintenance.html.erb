<div class="bod">
  <div class="col-md-10 col-sm-8 body-wrap">
<div class="set">
<div class="body-container">
<div class="body-header">
Trash - Machine Maintenance
</div>
<div class="body-content">
<div class="table-container">
<div class="header">
    List of Machine Maintenance
</div>
<table class="table">
<thead>
<tr>
<th>Machine No</th>
<th>Type of problem</th>
<th>Spare name</th>
<th>Spare cost</th>
<th colspan="3"><center>Actions</center></th>
</tr>
</thead>
<tbody>
<ul id = "machine" >
<% @machine_maintenance.each do |i| %>
<tr>
<td><div class="fot"><%= i.mach_no%></div></td>
<td><div class="fot"><%= i.type_of_prob%></div></td>
<td><div class="fot"><%= i.spare_name%></div></td>
<td><div class="fot"><%= i.spare_cost%></div></td>
<td><%= link_to image_tag("/assets/trash1.png", height: '20', width: '20',:class=>"img-circle",:title => 'Remove', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'delete_trashMachine', :id => i.id }%>
</td>
<td><center><%= link_to     users_trash_showMachineMaintenance_path(:id => i.id) ,class: "btn btn-default",:title => 'Show', :onclick => 'return openPopup(this);', :rel => 'tooltip' do%><i class="fa fa-eye"></i><%end%>
</center></td>


<td><%= link_to image_tag("/assets/restore.png", height: '20', width: '20',:class=>"img-circle",:title => 'Restore', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'restore_trashMachine', :id => i.id }%>
</td>

</tr>
<%end%>
</ul>
</tbody>
</table>
</div>
</div>
<div class="digg_pagination" align="right">
    <%= will_paginate @machine_maintenance1, :container => false %>
</div>
</div>
</div>
</div>
